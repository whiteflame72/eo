<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.eolinc.dto.Subject" table="subject">
        <id name="subjectId" type="long" column="SUBJECT_ID" >
            <generator class="native" />
        </id>
        <property name="subjectName" column="SUBJECT_NAME" length="45" not-null="true" />
     </class>
</hibernate-mapping>
